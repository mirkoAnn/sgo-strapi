      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "colors": {
      "type": "component",
      "component": "common.colors",
      "repeatable": false
    },
    "seo": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    },
    "slots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slot.slot",
      "mappedBy": "provider"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "providers"
