{
  "kind": "collectionType",
  "collectionName": "slots",
  "info": {
    "singularName": "slot",
    "pluralName": "slots",
    "displayName": "Slot"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gameplay": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sessions": {
      "type": "integer",
      "default": 0
    },
    "introContent": {
      "type": "blocks"
    },
    "rulesContent": {
      "type": "blocks"
    },
    "bonusContent": {
      "type": "blocks"
    },
    "opinionContent": {
      "type": "blocks"
    },
    "info": {
      "type": "component",
      "component": "slot.info",
      "repeatable": false
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "slots"
    },
    "slotThemes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::slot-theme.slot-theme",
      "inversedBy": "slots"
    },
    "provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::provider.provider",
      "inversedBy": "slots"
    },
    "gameUrl": {
      "type": "string"
    }
  }
}
